{
  "name": "ngx-page-builder",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4321 -o",
    "build": "ng build --project=demo",
    "build:sass": "sass projects/ngx-page-builder/src/assets/style.scss projects/ngx-page-builder/src/assets/styles.css",
    "build:lib": "npm run build:sass && ng build --project=ngx-page-builder",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write \".//projects//**//*.{ts,html,css,scss}\" ",
    "build:icons": "node ./projects/page-builer/scripts/build-icons.js"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^21.0.1",
    "@angular/common": "^21.0.1",
    "@angular/compiler": "^21.0.1",
    "@angular/core": "^21.0.1",
    "@angular/forms": "^21.0.1",
    "@angular/material": "^21.0.1",
    "@angular/platform-browser": "^21.0.1",
    "@angular/router": "^21.0.1",
    "@tinymce/tinymce-angular": "^9.1.1",
    "@zumer/snapdom": "^2.0.2",
    "highcharts": "^12.4.0",
    "highcharts-angular": "^5.2.0",
    "html-element-to-image": "^2.1.1",
    "html-to-image": "^1.11.13",
    "msgpack-lite": "^0.1.26",
    "ngx-page-builder": "^5.2.0",
    "ngx-input-color": "^5.0.0",
    "rxjs": "~7.8.0",
    "tinymce": "^8.2.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/msgpack-lite": "^0.1.12",
    "baseline-browser-mapping": "^2.9.18",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^21.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  }
}
